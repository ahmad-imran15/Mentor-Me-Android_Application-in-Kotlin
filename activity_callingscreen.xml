<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background1"
    >


    <FrameLayout
        android:id="@+id/remote_user"
        android:layout_width="match_parent"
        android:layout_marginTop="50dp"
        android:layout_height="577dp"
        android:background="#CBCBCB"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" >

        <FrameLayout

            android:id="@+id/local_user"
            android:layout_width="197dp"
            android:layout_height="209dp"
            android:layout_marginTop="350dp"
            android:layout_marginStart="190dp"
            android:layout_marginBottom="16dp"
            android:background="@color/white"
            android:fontFamily="sans-serif-medium"

            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="25dp"

            tools:ignore="MissingConstraints" />
    </FrameLayout>

    <TextView
        android:id="@+id/leavebutton"
        android:layout_width="66dp"
        android:layout_height="61dp"
        android:layout_marginTop="723dp"
        android:layout_marginStart="295dp"

        android:clickable="true"
        android:onClick="backbookappointment"
        android:layout_marginEnd="28dp"
        android:background="@drawable/baseline_cancel_24"
        android:fontFamily="sans-serif-medium"
        android:textColor="@color/black"

        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.943"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="161dp"
        android:layout_height="67dp"
        android:fontFamily="sans-serif-medium"
        android:layout_marginStart="130dp"
        android:layout_marginTop="620dp"
        android:text="________"
        android:textColor="#9D9E9E"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/textView5"
        android:layout_width="113dp"
        android:layout_height="78dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_marginStart="140dp"

        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.449"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.039"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="00:00"
            android:textColor="@color/black"
            android:textSize="25dp"></TextView>
    </LinearLayout>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="69dp"
        android:layout_height="64dp"
        android:layout_marginTop="723dp"
android:layout_marginStart="20dp"
        android:background="@drawable/mute"

        android:fontFamily="sans-serif-medium"
        android:gravity="center"

        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintHorizontal_bias="0.326"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.947"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/joinbutton"
        android:layout_width="78dp"
        android:layout_height="68dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="723dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/novideo"

        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="25dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.947"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="72dp"
        android:layout_height="66dp"
        android:layout_marginTop="723dp"
        android:layout_marginStart="200dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/refresh"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.947"
        tools:ignore="MissingConstraints" />


</RelativeLayout>